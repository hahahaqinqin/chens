mixin header
	//- BEGIN HEADER
	nav.header.bg-header.dark(data-pages='header', data-pages-header='autoresize', data-pages-resize-class='light')
		.container.relative
			//- BEGIN LEFT CONTENT
			.pull-left.z-index-10
				//- BEGIN HEADER TOGGLE FOR MOBILE
				.visible-sm-inline.visible-xs-inline.menu-toggler.pull-right(data-pages='header-toggle', data-pages-element='#header')
					.one
					.two
					.three
			//- BEGIN CENTER CONTENT
			.pull-center
				.pull-center-inner
					.header-inner
						//- BEGIN LOGO
						img.alt(src='/front/assets/images/logo_black.png', width='152', height='21', data-src-retina='/front/assets/images/logo_black_2x.png', alt='')
						img.logo(src='/front/assets/images/logo_white.png', width='152', height='21', data-src-retina='/front/assets/images/logo_white_2x.png', alt='')
			.pull-right.relative.z-index-10.visible-sm-inline.visible-xs-inline
				.header-inner
					//- a.search-toggle(href='#', data-toggle='search')
						//- i.fs-14.pg-search
			//- END CENTER CONTENT
			//- BEGIN MENU
			#header.menu-content.clearfix(data-pages='menu-content', data-pages-direction='slideLeft')
				//- BEGIN HEADER CLOSE TOGGLE FOR MOBILE
				.pull-left
					a.text-black.link.padding-10.visible-xs-inline.visible-sm-inline.pull-right.m-t-10.m-b-10.m-r-10(href='#', data-pages='header-toggle', data-pages-element='#header')
						i.pg-close_line
				.pull-left.sm-full-width
					.header-inner
