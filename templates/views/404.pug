doctype html
html
	head
		meta(http-equiv='content-type', content='text/html;charset=UTF-8')
		meta(charset='utf-8')
		title Pages - Front End
		meta(name='viewport', content='width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no')
		link(rel='apple-touch-icon', href='admin/pages/ico/60.png')
		link(rel='apple-touch-icon', sizes='76x76', href='admin/pages/ico/76.png')
		link(rel='apple-touch-icon', sizes='120x120', href='admin/pages/ico/120.png')
		link(rel='apple-touch-icon', sizes='152x152', href='admin/pages/ico/152.png')
		link(rel='icon', type='image/x-icon', href='favicon.ico')
		meta(name='apple-mobile-web-app-capable', content='yes')
		meta(name='apple-touch-fullscreen', content='yes')
		meta(name='apple-mobile-web-app-status-bar-style', content='default')
		meta(content='', name='description')
		meta(content='', name='author')
		//- BEGIN PLUGINS
		link(href='/front/assets/plugins/pace/pace-theme-flash.css', rel='stylesheet', type='text/css')
		link(href='/front/assets/plugins/bootstrap/css/bootstrap.min.css', rel='stylesheet', type='text/css')
		link(href='/front/assets/plugins/font-awesome/css/font-awesome.css', rel='stylesheet', type='text/css')
		link(href='/front/assets/plugins/swiper/css/swiper.css', rel='stylesheet', type='text/css', media='screen')
		//- END PLUGINS
		//- BEGIN PAGES CSS
		link.main-stylesheet(href='/front/pages/css/pages.css', rel='stylesheet', type='text/css')
		link.main-stylesheet(href='/front/pages/css/pages-icons.css', rel='stylesheet', type='text/css')
		//- BEGIN PAGES CSS
	body.error-page
		.container-xs-height.full-height
			.row-xs-height
				.col-xs-height.col-middle
					.error-container.text-center
						h1.error-number 404
						h2.semi-bold Sorry but we couldnt find this page
						p
							| This page you are looking for does not exsist 
							a(href='#') Report this?
		.pull-bottom.sm-pull-bottom.full-width.relative
			.error-container
				.error-container-innner
					.m-b-30.sm-m-t-20.sm-p-r-15.sm-p-b-20.clearfix
						.col-sm-3.no-padding
							img.m-t-5(alt='', data-src='/front/assets/images/pages_icon.png', data-src-retina='/front/assets/images/pages_icon_2x.png', height='60', src='/front/assets/images/pages_icon.png', width='60')
						.col-sm-9.no-padding
							p
								small
									| Create a pages account. If you have a facebook account, log into it for this process.
									| Sign in with 
									a(href='#') Facebook
									|  or 
									a(href='#') Google
		//- END FOOTER
		//- BEGIN VENDOR JS
		script(type='text/javascript', src='/front/assets/plugins/jquery/jquery-1.11.1.min.js')
		script(type='text/javascript', src='/front/assets/plugins/swiper/js/swiper.jquery.min.js')
		script(type='text/javascript', src='/front/assets/plugins/velocity/velocity.min.js')
		script(type='text/javascript', src='/front/assets/plugins/velocity/velocity.ui.js')
		//- END VENDOR JS
		//- BEGIN PAGES FRONTEND LIB
		script(type='text/javascript', src='/front/pages/js/pages.frontend.js')
		//- END PAGES LIB
		//- BEGIN YOUR CUSTOM JS
		script(type='text/javascript', src='/front/assets/js/custom.js')
		//- END PAGES LIB

